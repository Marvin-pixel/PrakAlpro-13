data = ('Marvin Patric Theodorus', '71241135', 'Sleman, DI Yogyakarta')

nama, nim, alamat = data

print("NIM   :", nim)
print("NAMA  :", nama)
print("ALAMAT:", alamat)

print("NIM :", tuple(nim))

print("NAMA DEPAN:", tuple(nama.split()[0].lower()))

print("NAMA TERBALIK:", tuple(nama.split()[::-1]))
